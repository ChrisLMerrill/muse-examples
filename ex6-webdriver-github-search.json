{
  "type" : "steppedtest",
  "defaultVariables" : {
    "search1" : {
      "value" : "selenium",
      "type" : "string"
    },
    "search2" : {
      "value" : "muse",
      "type" : "string"
    },
    "search3" : {
      "value" : "muse test framework",
      "type" : "string"
    }
  },
  "step" : {
    "type" : "compound",
    "metadata" : {
      "description" : "Find the Muse site on GitHub and read the wiki"
    },
    "sources" : { },
    "children" : [ {
      "type" : "open-browser",
      "sources" : {
        "provider" : {
          "type" : "resource",
          "source" : {
            "value" : "local-provider",
            "type" : "string"
          }
        },
        "browser" : {
          "type" : "resource",
          "source" : {
            "value" : "firefox",
            "type" : "string"
          }
        }
      }
    }, {
      "type" : "goto-url",
      "sources" : {
        "URL" : {
          "value" : "http://github.com/",
          "type" : "string"
        }
      }
    }, {
      "type" : "store-variable",
      "sources" : {
        "name" : {
          "value" : "counter",
          "type" : "string"
        },
        "value" : {
          "value" : 1,
          "type" : "integer"
        }
      }
    }, {
      "type" : "while",
      "sources" : {
        "condition" : {
          "type" : "lessthan",
          "sourceMap" : {
            "left" : {
              "type" : "variable",
              "source" : {
                "value" : "counter",
                "type" : "string"
              }
            },
            "right" : {
              "value" : 4,
              "type" : "integer"
            }
          }
        }
      },
      "children" : [ {
        "type" : "callfunction",
        "sources" : {
          "id" : {
            "value" : "github-search-function",
            "type" : "string"
          },
          "search_term" : {
            "type" : "variable",
            "source" : {
              "type" : "add",
              "sourceList" : [ {
                "value" : "search",
                "type" : "string"
              }, {
                "type" : "variable",
                "source" : {
                  "value" : "counter",
                  "type" : "string"
                }
              } ]
            }
          }
        },
        "children" : [ ]
      }, {
        "type" : "increment-variable",
        "sources" : {
          "name" : {
            "value" : "counter",
            "type" : "string"
          }
        }
      } ]
    }, {
      "type" : "verify",
      "sources" : {
        "condition" : {
          "type" : "exists",
          "source" : {
            "type" : "element-linktext",
            "source" : {
              "value" : "ChrisLMerrill/muse",
              "type" : "string"
            }
          }
        }
      }
    }, {
      "type" : "click-element",
      "sources" : {
        "element" : {
          "type" : "element-xpath",
          "source" : {
            "value" : "//h3[contains(@class, 'repo-list-name')]/a",
            "type" : "string"
          }
        }
      }
    }, {
      "type" : "click-element",
      "sources" : {
        "element" : {
          "type" : "element-linktext",
          "source" : {
            "value" : "Wiki",
            "type" : "string"
          }
        }
      }
    }, {
      "type" : "close-browser",
      "sources" : { }
    } ]
  }
}